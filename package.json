{
  "name": "@20minutes/draft-convert",
  "version": "3.0.3",
  "description": "Extensibly serialize & deserialize Draft.js ContentState",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "module": "esm/index.js",
  "repository": "20minutes/draft-convert",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:umd",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build:umd": "wp",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf ./dist ./lib ./esm",
    "build-and-test": "yarn clean && yarn build && yarn test",
    "lint": "eslint src/ test/"
  },
  "files": [
    "types",
    "dist",
    "lib",
    "esm"
  ],
  "keywords": [
    "draft",
    "draftjs",
    "draft-convert",
    "convert"
  ],
  "author": "bbriggs@hubspot.com",
  "license": "Apache-2.0",
  "peerDependencies": {
    "draft-js": ">=0.7.0",
    "react": "^15.0.2 || ^16.0.0-rc || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.0.2 || ^16.0.0-rc || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "immutable": "~3.7.4",
    "invariant": "^2.2.1"
  },
  "devDependencies": {
    "@20minutes/eslint-config": "^1.2.7",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^10.1.0",
    "draft-js": "^0.11.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "7.37.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "6.1.2",
    "webpack": "^5.103.0",
    "webpack-nano": "^1.1.1"
  }
}
